<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="Theme">
		<!-- Default disabled alpha for widgets that set enabled/disabled alpha programmatically. -->
		<attr name="disabledAlpha" format="float" />
	</declare-styleable>
	
	<declare-styleable name="DeviceOrTaskButtonView">
		<attr name="android:text" format="string" />
		<attr name="android:src" format="integer" />
		<attr name="contentDescription" format="string" />
		<attr name="cardCornerRadius" format="dimension" />
		<attr name="backgroundTint" format="color" />
	</declare-styleable>
	
	<declare-styleable name="CategoryView">
		<attr name="android:text" format="string" />
		<attr name="android:textAppearance" format="reference" />
	</declare-styleable>
	
	<declare-styleable name="IconTextView">
		<attr name="android:text" format="string" />
		<attr name="android:textAppearance" format="reference" />
		<attr name="android:icon" format="integer" />
		<attr name="android:iconTint" format="color" />
	</declare-styleable>
	
	<declare-styleable name="NumberPickerView">
		<attr name="android:hint" format="string" />
		<attr name="initialNumber" format="float" />
		<attr name="numberInterval" format="float" />
		<attr name="prefixText" format="string" />
		<attr name="suffixText" format="string" />
		<attr name="android:textAppearance" format="reference" />
		<attr name="numberAlignment" format="enum">
			<enum name="left" value="0" />
			<enum name="center" value="1" />
			<enum name="right" value="2" />
			<enum name="start" value="3" />
			<enum name="end" value="4" />
		</attr>
	</declare-styleable>
</resources>